
{
    "swagger": "2.0",
    "info": {
        "title": "Documentantion of My commands",
        "description": "Created by Bryan Tamayo",
        "version": "0.0.1"
    },
    "basePath": "/api/v1",
    "tags": [
        {
            "name": "Users"
        },
        {
            "name": "Info page"
        }
    ],
    "paths": {
        "/users/login": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Login",
                "description": "Without token. Prevously, put {process.env.PATH_ADMIN}/api/v1 in the endpoint.",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "schema": {
                            "type": "object",
                            "required": [
                                "email",
                                "password"
                            ],
                            "properties": {
                                "email": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string"
                                },
                                "data": {
                                    "type": "object",
                                    "properties": {
                                        "userName": {
                                            "type": "string"
                                        },
                                        "email": {
                                            "type": "string"
                                        },
                                        "rol": {
                                            "type": "string",
                                            "enum" : [
                                                "admin",
                                                "user"

                                            ]
                                        },
                                        "xen": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/users/register": {
            "post": {
                "tags": [
                    "Users"
                ],
                "summary": "Create a user",
                "description": "Without token. Prevously, put {process.env.PATH_ADMIN}/api/v1 in the endpoint.",
                "parameters": [
                    {
                        "in": "body",
                        "name": "body",
                        "schema": {
                            "type": "object",
                            "required": [
                                "userName",
                                "password",
                                "passwordConfirm",
                                "email"
                            ],
                            "properties": {
                                "userName": {
                                    "type": "string"
                                },
                                "password": {
                                    "type": "string"
                                },
                                "passwordConfirm": {
                                    "type": "string"
                                },
                                "email": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/infopage": {
            "get": {
                "tags": [
                    "Info page"
                ],
                "description": "Increment count in 1 of count of views web page. Counters to each month.",
                "responses": {
                    "200": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "status": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}